# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token

# OpenRouter API Key
OPENROUTER_API_KEY=your_openrouter_api_key

# Bot Owner (required) - Discord user ID of the bot owner
# The owner can manage admins via /admin commands
OWNER_USER_ID=123456789012345678

# Admin User IDs (deprecated - use /admin commands instead)
# Legacy support: admins defined here will be migrated to data/admins.json
ADMIN_USER_IDS=

# Rate limit: requests per user per minute
RATE_LIMIT_PER_MINUTE=5

# OpenRouter Model
OPENROUTER_MODEL=openai/gpt-5.1
